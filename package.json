{
  "name": "kratos-proto-generator",
  "displayName": "Kratos Proto Generator",
  "description": "Generate proto files for Kratos framework",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:kratos-proto-generator.generateProto"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kratos-proto-generator.generateProto",
        "title": "Generate Proto"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceFilename =~ /\\.proto$/ && resourcePath =~ /^api/",
          "command": "kratos-proto-generator.generateProto",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  }
} 